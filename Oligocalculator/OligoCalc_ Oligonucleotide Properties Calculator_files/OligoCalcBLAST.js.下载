function Blast2(){	if (document.OligoCalc.oligoBox.value.length < 8) {		alert("Please enter at least 8 bases before trying a BLAST search!");		return false;	}	blastwindow=window.open("OligoCalc2BLAST.html","BLAST2","toolbar=no,directories=no,status=yes,location=yes,scrollbars=yes,resizable=1");	return false;}function BLASTprepare(theForm) {	var theSeq="";	var theMoleculeType="DNA";	if (opener && opener.document.OligoCalc) {		var remoteForm=opener.document.OligoCalc;		theSeq=remoteForm.oligoBox.value;	} else {		theSeq=GetOligoCookie("");	}	if (theSeq.length < 8) {		alert("Please enter at least 8 bases before trying BLAST!");		return false;	}	theForm.QUERY.value=theSeq;	theForm.submit(); // comment out to stop submission!	return false;}